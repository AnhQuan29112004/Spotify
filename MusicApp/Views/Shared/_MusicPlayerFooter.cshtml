
@{
    ViewBag.Title = "_MusicPlayerFooter";
}

@Scripts.Render("~/Scripts/global.js")
<div class="footer">
    <div class="footer-left" style="width:20%">
        <div class="image">
            <img src="data:image/jpg;base64," alt="album-art" class="album-art">
        </div>
        <div class="song-info">
            <div class="song-title">Tình ca bất hủ</div>
            <div class="artist-name">Pháp sư</div>
        </div>
        <div><i class="bi bi-plus-circle"></i></div>
    </div>
    <div class="footer-center">
        <div style="display: flex; align-items: center">
            <button class="control-btn btn-child"><i class="bi bi-skip-start-fill"></i></button>
            <button class="control-btn" id="play-btn"><i class="bi bi-play-circle-fill" style=" font-size: 30px;"></i></button>
            <button class="control-btn btn-child"><i class="bi bi-skip-end-fill"></i></button>
        </div>
        <div class="progress-bar">
            <div class="current-time">0</div>
            <div class="range-input"><input type="range" class="progress-input" min="0" max="100"></div>
            <div class="total-time">1</div>
        </div>
    </div>
    <div class="footer-right">
        <i class="bi bi-repeat"></i>
        <div class="volume">
            <div><i class="bi bi-volume-mute"></i></div>
            <div class="range-input"><input type="range" class="volume-input" min="0" max="100" value="100"></div>
        </div>
    </div>
</div>

<script>


    $(document).ready(function () {
        window.addEventListener('pageshow', function (event) {
            // Check if navigating back (from bfcache)
            if (event.persisted) {
                let isPlaying = false;
                const data = JSON.parse(sessionStorage.getItem('currentSong'));
                const currentTime = JSON.parse(sessionStorage.getItem('currentTime'));
                const savedVolume = sessionStorage.getItem('audioVolume') || 0.5; // Default to 50%

                console.log('current', currentTime)
                console.log(data);
                if (data) {
                  
                    // Update the current song ID
                    currentSongId = data.id;

                    // Update footer with the current song's details
                    $('.album-art').attr('src', "/Public/Images/" + data.albumArt);
                    $('.song-title').text(data.title);
                    $('.artist-name').text(data.artist);

                    // Set audio source
                    audio.preload = "auto";
                    audio.src = "/Public/Songs/" + data.src;
                    audio.load();
                    // Update the total time based on the audio duration
                    audio.addEventListener('loadedmetadata', function () {
                        $('.total-time').text(formatTime(audio.duration));
                        $('.progress-input').attr('max', audio.duration);

                    });

                    if (currentTime) {

                        audio.currentTime = currentTime;
                    };

                    audio.volume = savedVolume;

                    // Update volume slider to match saved volume
                    $('.volume-input').val(audio.volume * 100);

                    // Play the song
                    isPlaying = true;

                    // Update sessionStorage with the current time during playback
                    audio.addEventListener("timeupdate", function () {
                        sessionStorage.setItem("currentTime", audio.currentTime);
                    });

                    $('.volume-input').on('input', function () {
                        const newVolume = $(this).val() / 100; // Convert slider value to 0-1 range
                        audio.volume = newVolume;
                        sessionStorage.setItem('audioVolume', newVolume); // Save to sessionStorage
                    });

                    // Show the footer if hidden
                    $('#musicPlayerFooter').show();

                    // Update the control button to pause
                    $('#play-btn i').removeClass('bi-pause-circle-fill').addClass('bi-play-circle-fill');

                    setInterval(() => {
                        if (isPlaying) {
                            $('.current-time').text(formatTime(audio.currentTime));
                            $('.progress-input').val(audio.currentTime);
                            updateRangeBackground($('.progress-input'));
                            updateRangeBackground($('.volume-input'));
                        }
                    }, 1000);


                }
            }
        });
        const $rangeInputs = $('input[type="range"]');

        $rangeInputs.each(function () {
            updateRangeBackground($(this)); // Initial background update
        });

        $rangeInputs.on('input', function () {
            updateRangeBackground($(this));
        });

        function updateRangeBackground($rangeElement) {
            const min = $rangeElement.attr('min') || 0;
            const max = $rangeElement.attr('max') || 100;
            const value = ($rangeElement.val() - min) / (max - min) * 100;
            $rangeElement.css('--val', value + '%'); // Update custom CSS variable

        }
        // Listen for the playSong event
        audio.addEventListener('ended', function () {
            // Get next song index
            currentSongIndex = currentSongIndex + 1;
            console.log(currentSongIndex);
            if (currentSongIndex >= window.listSong.length) {
                currentSongIndex = 0;
            }

            // Find and trigger click on next song element
            const nextSongElement = document.querySelector(`[data-index="${currentSongIndex}"]`);
            if (nextSongElement) {
                nextSongElement.click(); // This will trigger togglePlayPause
            }
        });

        let currentSongIndex = 0;
        let isPlaying = false;
        const data = JSON.parse(sessionStorage.getItem('currentSong'));
        const currentTime = JSON.parse(sessionStorage.getItem('currentTime'));
        const savedVolume = sessionStorage.getItem('audioVolume') || 0.5; // Default to 50%

        console.log('current', currentTime)
        console.log(data);
        if (data) {
            // Update the current song ID
            currentSongId = data.id;

            // Update footer with the current song's details
            $('.album-art').attr('src', "/Public/Images/" + data.albumArt);
            $('.song-title').text(data.title);
            $('.artist-name').text(data.artist);

            // Set audio source
            audio.preload = "auto";
            audio.src = "/Public/Songs/" + data.src;
            audio.load();
            // Update the total time based on the audio duration
            audio.addEventListener('loadedmetadata', function () {
                $('.total-time').text(formatTime(audio.duration));
                $('.progress-input').attr('max', audio.duration);

            });

            if (currentTime) {

                audio.currentTime = currentTime;
            };

            audio.volume = savedVolume;

            // Update volume slider to match saved volume
            $('.volume-input').val(audio.volume * 100);

            // Play the song

            isPlaying = true;

            // Update sessionStorage with the current time during playback
            audio.addEventListener("timeupdate", function () {
                sessionStorage.setItem("currentTime", audio.currentTime);
            });

            $('.volume-input').on('input', function () {
                const newVolume = $(this).val() / 100; // Convert slider value to 0-1 range
                audio.volume = newVolume;
                sessionStorage.setItem('audioVolume', newVolume); // Save to sessionStorage
            });

            // Show the footer if hidden
            $('#musicPlayerFooter').show();

            // Update the control button to pause
            $('#play-btn i').removeClass('bi-pause-circle-fill').addClass('bi-play-circle-fill');

            setInterval(() => {
                if (isPlaying) {
                    $('.current-time').text(formatTime(audio.currentTime));
                    $('.progress-input').val(audio.currentTime);
                    updateRangeBackground($('.progress-input'));
                    updateRangeBackground($('.volume-input'));
                }
            }, 1000);


        }

        // Handle pause functionality
        $('#play-btn').off('click').on('click', function () {
            if (audio.paused) {
                audio.play().catch(err => console.warn('Play blocked:', err));
                console.log('audio play')
                $('#play-btn i').removeClass('bi-play-circle-fill').addClass('bi-pause-circle-fill');
                $(document).trigger('footerSongPlaying'); // Notify that the song is playing

            } else {
                audio.pause();
                console.log('audio pause')
                $('#play-btn i').removeClass('bi-pause-circle-fill').addClass('bi-play-circle-fill');
                $(document).trigger('footerSongPaused'); // Notify that the song is paused
            }
        });

        // Handle progress bar change
        $('.progress-input').on('input', function () {
            audio.currentTime = $(this).val();
        });

        // Handle volume control
        $('.volume-input').on('input', function () {
            audio.volume = $(this).val() / 100; // Convert to 0-1 range
        });
        $(document).on('pauseSong', function () {
            audio.pause();
            $('#play-btn i').removeClass('bi-pause-circle-fill').addClass('bi-play-circle-fill');
            $(document).trigger('footerSongPaused'); // Notify that the song is paused
        });
        $(document).on('playSong', function () {
            audio.play();
            $('#play-btn i').removeClass('bi-play-circle-fill').addClass('bi-pause-circle-fill');
            $(document).trigger('footerSongPlay'); // Notify that the song is paused
        });
   
        $(document).on('playSongFirst', function (event) {
            if (window.songIndex != null) {
                currentSongIndex = window.songIndex;
            }
            // Update the current song ID
            let data = JSON.parse(sessionStorage.getItem('currentSong'));
            const currentTime = JSON.parse(sessionStorage.getItem('currentTime'));
            console.log('data: ', data)
            currentSongId = data.id;

            // Update footer with the current song's details
            $('.album-art').attr('src', "/Public/Images/" + data.albumArt);
            $('.song-title').text(data.title);
            $('.artist-name').text(data.artist);

            // Set audio source
            audio.preload = "auto";
            audio.src = "/Public/Songs/" + data.src;
            audio.load();
            // Update the total time based on the audio duration
            audio.addEventListener('loadedmetadata', function () {
                $('.total-time').text(formatTime(audio.duration));
                $('.progress-input').attr('max', audio.duration);

            });

            if (currentTime) {

                audio.currentTime = currentTime;
            };

            $('.volume-input').on('input', function () {
                const newVolume = $(this).val() / 100; // Convert slider value to 0-1 range
                audio.volume = newVolume;
                sessionStorage.setItem('audioVolume', newVolume); // Save to sessionStorage
            });

            // Play the song

            audio.play();
            isPlaying = true;

            // Update sessionStorage with the current time during playback
            audio.addEventListener("timeupdate", function () {
                sessionStorage.setItem("currentTime", audio.currentTime);
            });

            // Show the footer if hidden
            $('#musicPlayerFooter').show();

            // Update the control button to pause
            $('#play-btn i').removeClass('bi-play-circle-fill').addClass('bi-pause-circle-fill');

            setInterval(() => {
                if (isPlaying) {
                    $('.current-time').text(formatTime(audio.currentTime));
                    $('.progress-input').val(audio.currentTime);
                    updateRangeBackground($('.progress-input'));
                    
                }
            }, 1000);



        });
        $('.bi-skip-start-fill').click(function () {
            currentSongIndex = currentSongIndex - 1;
            if (currentSongIndex < 0) {
                currentSongIndex = 0;
            } else {
                const prevSongElement = document.querySelector(`[data-index="${currentSongIndex}"]`);
                if (prevSongElement) {
                    prevSongElement.click();
                }
            }
        });

        $('.bi-skip-end-fill').click(function () {
            currentSongIndex = currentSongIndex + 1;
            console.log(currentSongIndex);
            if (currentSongIndex >= window.listSong.length) {
                currentSongIndex = 0;
            }
            const nextSongElement = document.querySelector(`[data-index="${currentSongIndex}"]`);
            if (nextSongElement) {
                nextSongElement.click();
            }

        });

        function formatTime(seconds) {
            const minutes = Math.floor(seconds / 60);
            const secs = Math.floor(seconds % 60);
            return `${minutes}:${secs < 10 ? '0' : ''}${secs}`;
        }
    });

</script>